{
  "name": "react-bootstrap-table-next",
  "version": "4.2.2",
  "private": true,
  "type": "module",
  "description": "Rebuilt for react-bootstrap-table",
  "main": "index.js",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "build": "gulp prod",
    "lint": "eslint ./packages --ext .js --ext .jsx --ignore-path .gitignore",
    "pretest": "yarn lint --cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "storybook": "cd ./packages/react-bootstrap-table-next-example && yarn storybook",
    "gh-pages:clean": "cd ./packages/react-bootstrap-table-next-example && yarn gh-pages:clean",
    "gh-pages:build": "cd ./packages/react-bootstrap-table-next-example && yarn gh-pages:build",
    "release": "yarn install && yarn build && lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SujiJoe/npm-libs.git#react-bootstrap-table"
  },
  "author": "AllenFang",
  "contributors": [
    "Allen Fang <ayu780129@hotmail.com> (https://github.com/AllenFang)",
    "Chun-MingChen <nick830314@gmail.com> (https://github.com/Chun-MingChen)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-bootstrap-table-next/issues"
  },
  "homepage": "https://github.com/react-bootstrap-table-next#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "9.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "0.28.1",
    "dart-sass": "^1.25.0",
    "enzyme": "3.4.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-shell": "^0.8.0",
    "html-webpack-plugin": "5.5.1",
    "jest": "20.0.4",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "lerna": "2.8.0",
    "react-test-renderer": "17.0.0",
    "sass-loader": "13.2.2",
    "sinon": "3.2.1",
    "style-loader": "0.17.0",
    "webpack": "5.20.0",
    "webpack-dev-server": "4.13.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "15.5.10",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "underscore": "1.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.js",
      "packages/*/index.js",
      "!packages/react-bootstrap-table-next-example/**/*.js"
    ],
    "roots": [
      "<rootDir>/packages"
    ],
    "setupFiles": [
      "<rootDir>/enzyme-setup.js"
    ],
    "modulePaths": [
      "<rootDir>/packages/react-bootstrap-table-next"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "directories": {
    "doc": "docs"
  }
}
